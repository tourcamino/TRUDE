{
  "version": 2,
  "name": "trude-defi-app",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["prisma/schema.prisma"]
      }
    }
  ],
  "routes": [
    {
      "src": "/trpc/(.*)",
      "dest": "/api/trpc/$1"
    },
    {
      "src": "/api/debug/client-logs",
      "dest": "/api/debug/client-logs"
    },
    {
      "src": "/api/health",
      "dest": "/api/health"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "functions": {
    "api/**/*.ts": {
      "maxDuration": 30
    }
  },
  "env": {
    "NODE_ENV": "production",
    "DB_FALLBACK_MEM": "1",
    "FACTORY_ADDRESS": "0x0000000000000000000000000000000000000000"
  },
  "buildCommand": "npm run build",
  "outputDirectory": ".vinxi/build",
  "installCommand": "npm install --legacy-peer-deps",
  "framework": null
}